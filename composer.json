{
    "name": "advancedhosters/hipanel-theme-adminlte",
    "type": "yii2-extension",
    "description": "AdvancedHosters AdminLte Theme for HiPanel",
    "keywords": [
        "advancedhosters",
        "adminlte",
        "theme",
        "hipanel",
        "plugin"
    ],
    "homepage": "https://github.com/advancedhosters/hipanel-theme-adminlte",
    "license": "No license",
    "support": {
        "email": "support@advancedhosters.com",
        "source": "https://github.com/advancedhosters/hipanel-theme-adminlte",
        "issues": "https://github.com/advancedhosters/hipanel-theme-adminlte/issues",
        "wiki": "https://github.com/advancedhosters/hipanel-theme-adminlte/wiki"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "email": "sol@hiqdev.com",
            "homepage": "https://hipanel.com/"
        }
    ],
    "require": {
        "hiqdev/hipanel-theme-adminlte": "*@dev",
        "advancedhosters/yii2-asset-advancedhosters": "*@dev"
    },
    "autoload": {
        "psr-4": {
            "advancedhosters\\themes\\adminlte\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
